  <version>0.9.3</version>
  <description>3Laws Robot Diagnostic Module - ROS2</description>
  <maintainer email="tgurriet@3laws.io">Thomas Gurriet</maintainer>
  <license>Exclusive property of 3Laws Robotics Inc.</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>lll_rdm_msgs</depend>
  <depend>lll_supervisor_msgs</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_cmake_gtest</test_depend>
